# ASR配置
ASR_MODEL_NAME=paraformer-zh
ASR_DEVICE=cpu

# Whisper配置
WHISPER_MODEL_NAME=large-v3-turbo
WHISPER_DEVICE=cpu
HF_TOKEN=your_huggingface_token_here

# 说话人识别配置
SPEAKER_THRESHOLD=0.5
SPEAKER_DEVICE=cpu

# 向量数据库配置
VECTOR_DB_TYPE=CHROMADB
VECTOR_DB_PERSIST_DIRECTORY=./voice_vectors
VECTOR_DB_COLLECTION_NAME=voice_prints

# S3存储配置
STORAGE_TYPE=S3
S3_BUCKET_NAME=voice
S3_ENDPOINT_URL=http://localhost:9000
S3_ACCESS_KEY_ID=admin
S3_SECRET_ACCESS_KEY=minioadmin123
S3_PREFIX=stt

# 服务配置
TRANSCRIPTION_STRATEGY=AUTO_SELECT
MAX_TRANSCRIBE_CONCURRENT=2
MAX_SPEAKER_CONCURRENT=3
TASK_TIMEOUT=300.0

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8765
SERVER_LOG_LEVEL=info